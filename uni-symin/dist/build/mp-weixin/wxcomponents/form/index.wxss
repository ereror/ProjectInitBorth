.f-box {
	display: flex;
	width: 690rpx;
	margin: 0 auto;
	justify-content: space-between;
	align-items: center;
	height: 120rpx;
	background: white;
	padding: 0 30rpx;
	border-bottom: 1rpx solid #eeeeee;
}

/* .f-box:last-child {
	border-bottom: 0;
} */
.isborderR {
	border-radius: 30rpx;
}

.f-icon {
	height: 42rpx;
	width: 42rpx;
	margin-right: 10rpx;
}

.f-title {
	font-size: 30rpx;
	flex: 1;
}

.f-content, .f-ipt {
	font-size: 30rpx;
	flex-grow: 1;
	flex: 1;
	font-weight: 600;
	padding: 0 10rpx;
	text-align: right;
}

.f-ipt {
	border: 0;
	background: 0;
	outline: 0;
}

.text-placeholder {
	color: #a1a1a1;
	font-size: 28rpx;
	font-weight: 500;
}

.f-arrow {
	transform: rotate(180deg);
	width: 16rpx;
	height: 25rpx;
}