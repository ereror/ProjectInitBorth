
<view class="cu-custom" style="height:{{barconfig.CustomBar}}px;">
  <view class="cu-bar, fixed, {{barconfig.bgset}}" style="height:{{barconfig.CustomBar}}px;padding-top:{{barconfig.StatusBar}}px;">
	<navigator class='action' open-type="navigate" url="/pages/release/release" delta="1" hover-class="none" wx:if='{{barconfig.hiddenTitle === 2}}' style="height:{{barconfig.CustomBar - barconfig.StatusBar}}px;">
	  <view style="height:{{barconfig.CustomBar - barconfig.StatusBar}}px;"></view>
	  <image src="{{barconfig.isArrow ?barconfig.isArrow === 1 ? iconb : iconbw : ''}}" class="actionimage1" wx:if='{{barconfig.isArrow}}'/>
	</navigator>
	<navigator class='action' delta="1" open-type="navigateBack" hover-class="none" wx:if='{{barconfig.hiddenTitle === 1}}' style="height:{{barconfig.CustomBar - barconfig.StatusBar}}px;">
	  <view style="height:{{barconfig.CustomBar - barconfig.StatusBar}}px;"></view>
	  <image src="data:image/png;base64," class="actionimage2"/>
	</navigator>
	<view class='cu-custometitle {{barconfig.tset}}'>{{barconfig.title}}</view>
  </view>
</view>