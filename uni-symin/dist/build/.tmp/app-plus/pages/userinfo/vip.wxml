<block><custombar vue-id="1" name="uni-app" barconfig="{{barconfig}}" data-com-type="wx" bind:__l="__l"></custombar><view class="vip-head"><image class="viph" src="{{userInfo.header_url?userInfo.header_url:viph}}" mode="aspectFill"></image></view><view class="vip-box"><image class="vip-box-img" src="{{vipimg}}"></image><view class="vip-range">{{userInfo.vip_id?vipText[userInfo.vip_id]:'普通会员'}}</view><view class="vip-count">{{"每天"+(!userInfo.vip_id?5:vipText[-userInfo.vip_id])+"把钥匙"}}</view><view class="vip-title">购买vip</view><view class="vip-itembox"><block wx:for="{{vipItem}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="vip-item" data-id="{{item.id}}" data-event-opts="{{[['tap',[['checkOrder',['$event']]]]]}}" bindtap="__e"><image class="vip-dot" mode="aspectFill" src="{{vipIcon[index]}}"></image><view class="vip-item-content"><view class="vip-rang"><text>{{item.title}}</text><view class="vip-datas"><image class="vipt" src="{{vipt}}" mode="widthFix"></image><text>{{item.times}}</text></view><block wx:if="{{item.isTj}}"><image class="vipmore" src="{{vipmore}}" mode="widthFix"></image></block></view><view class="vip-tips">{{item.text}}</view><block wx:if="{{item.lititle}}"><view class="vip-tips">{{item.lititle}}</view></block></view><button class="c-btn vipbtn"><text>￥</text><text class="vip-text-big">{{item.price+"."}}</text><text>00</text></button></view></block></view><view class="vip-title vip-title2">会员权限</view><view class="vip-server"><block wx:for="{{vips}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="vip-s-item"><image class="vip-s-img" src="{{item.img}}"></image><view class="vip-s-text">{{item.text}}</view></view></block></view></view></block>