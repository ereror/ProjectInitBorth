.swiper-container{
  position: relative;
  border-radius: 30rpx;
  overflow: hidden;
  background: none;
  min-height: 320rpx;
}
.swiper-container .swiper{
  width: 690rpx;
  height: 690rpx;
  margin: 0 auto;
  border-radius:30rpx;
  overflow: hidden;
}

.swiper-container .swiperli {
	height: 260rpx;
	width: 750rpx;
}
.swiper-container .swiper .img{
  width: 100%;
  height: 100%;
  border-radius: 30rpx;
}

.swiper-container .swiperli .imgR .img{
	margin: 0 auto;
	width: 690rpx;
}
.swiper-container .dots, .swiper-container .dotIndex{
  position: absolute;
  left: 0;
  right: 0;
  bottom: 20rpx;
  display: flex;
  justify-content: center;
  z-index:9;
}
.swiper-container .dots .dot{
  margin: 0 8rpx;
  width: 14rpx;
  height: 14rpx;
  background: #fff;
  border-radius: 8rpx;
}
.swiper-container .dots .dot.active{
  width: 30rpx;
  background: #f81f5e;
}

.imgR {
	border-radius: 30rpx;
	overflow: hidden;
}

.swiper-container .dotIndex .dot{
	height: 12rpx;
	width: 12rpx;
	border-radius: 6rpx;
	background: #ffb400;
	margin-right: 30rpx;
}

.swiper-container .dotIndex .dot.active{
	width: 30rpx;
	background: #f81f5e;
	border: 2rpx solid #F9F9FB;
}
