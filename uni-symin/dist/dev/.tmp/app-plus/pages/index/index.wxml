/view><view class="I-d-icons flex-x-l"><view class="I-d-time I-textshadow">{{item.login_time||'1天前'}}</view><block wx:if="{{item.location.length}}"><view class="I-d-addr I-textshadow">{{item.location||'四川成都 天府广场'}}</view></block><block wx:if="{{item.distance>0.5}}"><view class="I-d-distance I-textshadow">{{item.distance+"km"}}</view></block><block wx:if="{{item.distance<0.5&&item.distance*1>0.001}}"><view class="I-d-distance I-textshadow">{{item.distance*1000+"m"}}</view></block><block wx:if="{{item.distance*1<0.001}}"><view class="I-d-distance I-textshadow">10m内</view></block><block wx:if="{{item.heart}}"><view class="I-d-msgIcon"><image class="I-d-msgIcon-img" src="{{vHot}}"></image></view></block><block wx:if="{{item.vip_id}}"><view class="I-d-msgIcon2 flex-c"><image class="I-d-msgIcon-img" src="{{vipIcon[item.vip_id-1]}}" mode="aspectFill"></image><view class="I-d-msgIcon2-text">{{vipItem[item.vip_id]||'普通'}}</view></view></block></view></view><block wx:if="{{item.video_url}}"><view class="I-d-rz">视频已认证</view></block></view><view class="I-text-box flex-b"><view class="I-text flex-x-l"><view class="I-text-mark">●</view><view class="I-text-detail">{{"希望一起"+(selfTypesObj[item.objective]||'吃饭')}}</view></view><view class="I-btn"></view></view></view></view></block><block wx:if="{{loading}}"><loading vue-id="3" loadConf="{{loadConf}}" data-com-type="wx" bind:__l="__l"></loading></block></view><tbar vue-id="4" isActiveIndex="0" tbarconfig="{{tbarconfig}}" data-com-type="wx" bind:__l="__l"></tbar></block>