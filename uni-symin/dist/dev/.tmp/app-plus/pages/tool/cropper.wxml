<view><view class="cropper-wrapper"><canvas class="cropper" style="{{'width:'+(cropperOpt.width+'px')+';'+('height:'+(cropperOpt.height+'px')+';')+('background-color:'+('rgba(0, 0, 0, 0.8)')+';')}}" disable-scroll="true" canvas-id="{{cropperOpt.id}}" data-event-opts="{{[['touchstart',[['touchStart',['$event']]]],['touchmove',[['touchMove',['$event']]]],['touchend',[['touchEnd',['$event']]]]]}}" bindtouchstart="__e" bindtouchmove="__e" bindtouchend="__e"></canvas><cover-view class="cropper-buttons-tip">图片只支持jpg,png,jpeg</cover-view><cover-view class="cropper-buttons"><cover-view data-event-opts="{{[['tap',[['uploadTap',['$event']]]]]}}" class="upload" bindtap="__e">选择图片</cover-view><cover-view data-event-opts="{{[['tap',[['saveCropperImage',['$event']]]]]}}" class="getCropperImage" bindtap="__e">完成</cover-view></cover-view></view></view>