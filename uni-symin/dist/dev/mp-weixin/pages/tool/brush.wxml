<block><custombar vue-id="1" name="uni-app" barconfig="{{barconfig}}" data-com-type="wx" bind:__l="__l"></custombar><block wx:if="{{showChoosebox}}"><dialogbox vue-id="2" items="{{items}}" boxconfig="{{boxconfig}}" choseKey="{{queryParam.choseKey}}" data-event-opts="{{[['^surefunc',[['surefunc']]],['^cancelfunc',[['cancelfunc']]]]}}" data-com-type="wx" bind:surefunc="__e" bind:cancelfunc="__e" bind:__l="__l" vue-slots="{{['default']}}"><block wx:if="{{showVip}}"><block><view class="b-content"><view class="b-title flex-c">VIP会员高级筛选</view><view class="b-title b-title-02 flex-c">高级筛选需要成为VIP</view></view></block></block></dialogbox></block><view data-event-opts="{{[['tap',[['resetfunc',['$event']]]]]}}" class="b-bar" bindtap="__e"><view class="b-text">重置</view><view class="iconfont icon-sync b-iconcz"></view></view><view class="b-bar-1"><choose vue-id="3" choArr="{{orderType}}" choCount="{{queryParam.sort}}" choTitle="排序" keys="1" data-event-opts="{{[['^setValue',[['sexfunc']]]]}}" data-com-type="wx" bind:setValue="__e" bind:__l="__l"></choose><choose vue-id="4" choArr="{{sexArr}}" choCount="{{queryParam.gender}}" choTitle="性别" keys="2" data-event-opts="{{[['^setValue',[['sexfunc']]]]}}" data-com-type="wx" bind:setValue="__e" bind:__l="__l"></choose><choose vue-id="5" choArr="{{checkArr}}" choCount="{{queryParam.video}}" choStyle="margin-bottom: 20rpx;" choTitle="视频认证" keys="3" data-event-opts="{{[['^setValue',[['sexfunc']]]]}}" data-com-type="wx" bind:setValue="__e" bind:__l="__l"></choose></view><view class="b-bar-2"><block wx:if="{{!isVip}}"><view data-event-opts="{{[['tap',[['showVipBox',['$event']]]]]}}" class="b-bar-col" bindtap="__e"></view></block><view class="b-bar-box"><view class="b-range"><view class="b-range-text"><view>年龄</view><view>{{queryParam.ageVal[0]+" ~ "+queryParam.ageVal[1]}}</view></view><view class="b-range-bar"><slide vue-id="6" width="{{ageWidth}}" value="{{queryParam.ageVal}}" min="18" max="70" thumb-style="{{thumbStyle}}" active-color="#f81f5e" slider-weight="2" data-event-opts="{{[['^change',[['getsliderValA']]]]}}" data-com-type="wx" bind:change="__e" bind:__l="__l"></slide></view></view><view class="b-range"><view class="b-range-text"><view>身高</view><view>{{queryParam.heightVal[0]+" ~ "+queryParam.heightVal[1]}}</view></view><view class="b-range-bar"><slide vue-id="7" width="{{ageWidth}}" value="{{queryParam.heightVal}}" min="150" max="220" thumb-style="{{thumbStyle}}" active-color="#f81f5e" slider-weight="2" data-event-opts="{{[['^change',[['getsliderValH']]]]}}" data-com-type="wx" bind:change="__e" bind:__l="__l"></slide></view></view><view class="b-choose"><iptitem vue-id="8" inputconfig="{{brushTool[0]}}" data-event-opts="{{[['^choose',[['chooseItem']]]]}}" data-com-type="wx" bind:choose="__e" bind:__l="__l"></iptitem><iptitem vue-id="9" inputconfig="{{brushTool[1]}}" data-event-opts="{{[['^choose',[['chooseItem']]]]}}" data-com-type="wx" bind:choose="__e" bind:__l="__l"></iptitem></view><view class="b-choose2"><view class="b-choose2-tite">最新</view><view class="b-choose"><iptitem vue-id="10" inputconfig="{{brushTool[2]}}" data-com-type="wx" bind:__l="__l" vue-slots="{{['default']}}"><switch data-key="1" color="#F81F5E" checked="{{queryParam.recentR}}" data-event-opts="{{[['change',[['switchChange',['$event']]]]]}}" bindchange="__e"></switch></iptitem></view></view><view class="b-choose3"><view class="b-choose3-tite">邀约项目</view><view class="b-choose3-box"><itemh vue-id="11" itemData="{{selfTypes}}" data-event-opts="{{[['^setValue',[['itemHfunc']]]]}}" data-com-type="wx" bind:setValue="__e" bind:__l="__l"></itemh></view></view></view></view><form class="uinfo-d-btnbox" report-submit="{{true}}" data-event-opts="{{[['submit',[['createFormId',['$event']]]]]}}" bindsubmit="__e"><button class="c-btn bg-red2 brush-btn" form-type="submit" data-event-opts="{{[['tap',[['goBack',['$event']]]]]}}" bindtap="__e">确定</button></form></block>