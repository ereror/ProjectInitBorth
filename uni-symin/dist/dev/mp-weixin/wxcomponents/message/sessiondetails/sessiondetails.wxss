/* 界面容器 */
page {
    width: 100%;
    height: 100vh;
    overflow: hidden;
}

.chatdetails {
    width: 100%;
    height: 100vh;
    background-image: url('data:image/png;base64,');
    background-size: 100% 100%;
}

/* 顶部动态导航 */
.cu-custom {
    display: block;
    position: relative;
    z-index: 10000;
}

.bg-gradual-default {
    background-color: white;
    color: #333333;
}

.bg-gradual-dred {
    background-color: #f81f5e;
    color: white;
}

/* .cu-custom .cu-bar {
  padding-right: 220rpx;
  box-shadow: 0rpx 0rpx 0rpx;
} */

.cu-custom .cu-bar {
    min-height: 0px;
    display: flex;
    justify-content: flex-start;
}

.cu-bar.fixed,
.nav.fixed {
    position: fixed;
    width: 100%;
    top: 0;
    z-index: 1024;
    /* box-shadow: 0 1rpx 6rpx rgba(0, 0, 0, 0.1); */
}

.cu-bar {
    display: flex;
    position: relative;
    align-items: center;
    min-height: 100rpx;
    justify-content: space-between;
}

.cu-bar .action:last-child {
    margin-right: 30rpx;
}

.cu-bar .action:first-child {
    font-size: 32rpx;
}

.action>view {
    width: 32rpx;
}

.cu-bar .action {
    display: flex;
    align-items: center;
    height: 100%;
    width: 100rpx;
    max-width: 100%;
}


.cu-bar .action:first-child>text {
    margin-left: -0.3em;
    margin-right: 0.3em;
}

.cu-custometitle {
    position: absolute;
    width: 750rpx;
    color: rgba(0, 0, 0, 1);
    /*  font-weight: bold; */
    font-size: 32rpx;
    z-index: -2;
    -webkit-background-clip: text;
    display: flex;
    align-items: center;
    justify-content: center;
}

.chatseting {
    position: absolute;
    width: 750rpx;
    height: 28rpx;
    bottom: 0;
    left: 0;
    z-index: 10001;
    display: flex;
    align-items: center;
    justify-content: center;
}

.chatseting_itransform {
    position: absolute;
    width: 750rpx;
    height: 60rpx;
    bottom: -60rpx;
    left: 0;
    z-index: 10002;
    display: flex;
    align-items: center;
    justify-content: center;
    background-color: rgba(248, 31, 94, .3);
    overflow: hidden;
    transition: .1s;
}

.chatseting_itransform>image {
    width: 58rpx;
    height: 16rpx;
}

.chatseting>view {
    width: 28rpx;
    height: 28rpx;
    position: relative;
}

.chatseting>view>view {
    display: inline-block;
    width: 0;
    height: 0;
    vertical-align: middle;
    border-bottom: 6rpx solid white;
    border-right: 6rpx solid transparent;
    border-left: 6rpx solid transparent;
    -webkit-transition: all .2s ease;
    transition: all .2s ease;
    position: absolute;
    top: 10rpx;
    left: 8rpx;
}

.itransform {
    transform: rotate(180deg);
}

.icon-180d {
    transform: rotate(-180deg)
}

.actionimage1 {
    width: 32rpx;
    height: 32rpx;
}

.actionimage2 {
    width: 24rpx;
    height: 35rpx;
}

.action {
    width: 200rpx !important;
}

.bg-gradual-tred {
    color: white;
}

/* 发送消息 */

/* 文字编辑 */
.sendmessagedz {
    width: 100%;
    height: 300rpx;
}

.sendmessage {
    width: 100%;
    min-height: 100rpx;
    position: fixed;
    bottom: 0;
    left: 0;
    background-color: white;
    box-shadow: 0px 0px 8px 0px #d0d0d0;
}

.sendmessage_text {
    width: 100%;
    height: 100rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.emige,
.go {
    width: 100rpx;
    height: 100rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}

.emige>image,
.go>image {
    width: 40rpx;
    height: 40rpx;
}

.inputmsg {
    width: 550rpx;
    min-height: 100rpx;
}

.inputmsg_nosr {
    width: 550rpx;
    height: 100rpx;
    display: flex;
    align-items: center;
    color: #999;
    font-size: 28rpx;
}

.inputmsg_textarea {
    width: 550rpx;
    min-height: 100rpx;
    display: flex;
    align-items: center;
}

.inputmsg_textarea>textarea {
    width: 100%;
    color: #000;
    font-size: 28rpx;
    line-height: 40rpx;
    /* background-color: red; */
    height: 80rpx;
}

.inputmsg_textarea>input {
    width: 100%;
    color: #000;
    font-size: 28rpx;
    height: 40rpx;
    line-height: 40rpx;
    /* background-color: green; */
}

.placeholder_input {
    color: #999999;
}

.owelineinput {
    padding: 22rpx 0;
    height: 40rpx !important;
}

.noowelineinput {
    padding: 0;
    height: 80rpx !important;
}

.gono {
    opacity: 0.4;
}

.goyes {
    opacity: 1;
}

/* 表情选择容器 */
.sendmessage_emoji {
    width: 100%;
}

.sendmessage_emoji_con {
    width: 100%;
    height: 518rpx;
    background-color: rgba(236, 238, 241, 1);
}

.sendmessage_emoji_con_title {
    color: #8F8E94;
    font-size: 24rpx;
    height: 50rpx;
    width: 100%;
    overflow: hidden;
    position: relative;
}

.sendmessage_emoji_con_title>view {
    padding-left: 32rpx;
    position: absolute;
    bottom: 0;
    left: 0;
}

.sendmessage_emoji_con_con1,
.sendmessage_emoji_con_con1_swiper,
.sendmessage_emoji_con_con1_swiper_item {
    width: 710rpx;
    height: 380rpx;
    /* background-color: #ccc; */
}

.sendmessage_emoji_con_con1 {
    margin: 0 auto;
}

.sendmessage_emoji_con_con1_swiper_item {
    display: flex;
    flex-wrap: wrap;
}

.sendmessage_emoji_con_con1_swiper_item_con {
    width: 88.75rpx;
    height: 76rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}

.sendmessage_emoji_con_con1_swiper_item_con>image {
    width: 56rpx;
    height: 56rpx
}

.sendmessage_emoji_con_con_this {
    width: 100%;
    height: 88rpx;
    display: flex;
    align-items: center;
}

.sendmessage_emoji_con_con_this_item {
    width: 70rpx;
    height: 70rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    border-radius: 50%;
}

.sendmessage_emoji_con_con_this_item:nth-child(1) {
    margin-left: 20rpx;
}

.sendmessage_emoji_con_con_this_item>image {
    width: 36rpx;
    height: 36rpx;
}

/* 弹窗 */
.reportposhow {
    width: 100%;
    height: 100vh;
    position: fixed;
    left: 0;
    top: 0;
    z-index: 99999;
    background-color: rgba(00, 00, 00, 0.3);
    display: flex;
    align-items: center;
    justify-content: center;
}

.reportposhow_content {
    width: 540rpx;
    height: 344rpx;
    border-radius: 30rpx;
    background-color: white;
}

.reportposhow_content_text {
    width: 100%;
    height: 89rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 32rpx;
    font-weight: 400;
    color: rgba(51, 51, 51, 1);
    border-bottom: 1rpx solid rgba(238, 238, 238, 1);
}

.reportposhow_content_button {
    width: 100%;
    height: 164rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}

.reportposhow_content_button>view {
    width: 200rpx;
    height: 88rpx;
    background: rgba(98, 103, 249, 1);
    border-radius: 44rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 32rpx;
    color: white;
    font-weight: 400;
}

.reportposhow_content_button>view:active {
    opacity: .6;
}