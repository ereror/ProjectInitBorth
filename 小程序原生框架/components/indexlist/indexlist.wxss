.index-float-block {
  transition: opacity 1s;
  opacity: 0;
  position: fixed;
  text-align: center;
  width: 100px;
  height: 100px;
  border-radius: 50px;
  background-color: rgba(0, 0, 0, 0.5);
  font-size: 50px;
  line-height: 100px;
  color: white;
}
.index-list-top {
  overflow: hidden;
}
.ui-index-list {
  position: relative;
  /*overflow: hidden;*/
  background-color: #FEFEFE;
}
.ui-index-list .index-selector-wrap {
  position: fixed;
  z-index: 2;
  font-size: 10px;
  right: 0px;
  text-align: center;
  width: 30px;
  color: #3399ff;
  border-radius: 5px;
  top: 0rpx;
  bottom: 0rpx;
}
.ui-index-list .index-selector-wrap .index-selector-item {
  width: 30px;
  text-align: center;
  position: absolute;
  height: 16px;
}
.ui-index-list .index-list-title {
  height: 24px;
  font-size: 10px;
  background-color: #f4f6f8;
  color: #808080;
  line-height: 24px;
  padding-left: 8px;
}
.ui-index-list .index-list-content {
  font-size: 14px;
  padding-left: 8px;
  height: 54px;
  line-height: 54px;
  border-bottom: #f4f6f8 solid 1px;
}
.ui-index-list .index-list-content-disabled {
  padding-left: 8px;
  height: 54px;
  line-height: 54px;
  background-color: #CCC;
  border-bottom: #f4f6f8 solid 1px;
}
.ui-index-list .index-list-button {
  float: right;
  margin-top: 12px;
  margin-right: 28px;
}
